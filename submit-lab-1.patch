-  reset_if_needed();
-
-  blk1 = rhmalloc(ALIGNMENT*10 + sizeof(metadata_t));
-  rhmalloc(ALIGNMENT);
-  entry2 = freelist_head()->s.next;
-  rhfree(blk1);
