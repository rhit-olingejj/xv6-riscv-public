-static void test_too_small_blocks(void)
-{
-  void *blk1, *blk1_v2;
-  metadata_t *entry1, *entry2, *split;
-
-  reset_if_needed();
-
-  blk1 = rhmalloc(ALIGNMENT);
-  rhmalloc(ALIGNMENT);
-
-  rhfree(blk1);
-  blk1_v2 = rhmalloc(ALIGNMENT + 1);
